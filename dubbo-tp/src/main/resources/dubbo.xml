<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--dubbo过滤器-->
    <dubbo:provider filter="tpFilter"/>
    <!--dubbo应用程序命名-->
    <dubbo:application name="dubbo-tp"/>
    <!--dubbo注册地址-->
    <dubbo:registry address="zookeeper://192.168.0.51:2181"/>
    <!--<dubbo:registry address="zookeeper://192.168.48.142:2181"/>-->
    <!--dubbo协议地址-->
    <dubbo:protocol name="dubbo" port="20884"/>
    <!--接口声明-->
    <dubbo:service interface="com.lagou.tp.service.TpService" ref="tpService"/>
    <bean id="tpService" class="com.lagou.tp.provider.TpServiceImpl"/>

</beans>