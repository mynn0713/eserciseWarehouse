<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!--dubbo应用程序命名-->
    <dubbo:application name="dubbo-demo-web"/>
    <!--dubbo注册地址-->
    <dubbo:registry address="zookeeper://192.168.0.51:2181"/>
    <!--dubbo协议地址-->
    <dubbo:protocol name="dubbo" port="20880"/>
    <!--接口引用-->
    <dubbo:reference interface="com.lagou.api.service.StudyService" id="studyService"/>
    <dubbo:reference interface="com.lagou.api.service.EatService" id="eatService"/>
</beans>